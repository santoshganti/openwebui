# Common
PUID=1000
PGID=996
TZ=America/Los_Angeles

# App
APPDATA=/mnt/zfspool/appdata/openwebui
APPSTORAGE=/mnt/zfspool/appstorage/openwebui
DATABASE=/mnt/zfspool/databases/openwebui
DOMAIN=darkcatalist.com
SERVICES_DOMAIN=services.darkcatalist.com

# Ollama Configuration
# OPENWEBUI_APP_IP_OLLAMA=192.168.6.131
OPENWEBUI_OLLAMA_HOST=openwebui-ollama
OPENWEBUI_OLLAMA_PORT=7869
OPENWEBUI_OLLAMA_KEEP_ALIVE=24h

# OpenWebUI - port 8080
OPENWEBUI_WEBUI_APP_IP=192.168.6.130
OPENWEBUI_WEBUI_PORT=8080
OPENWEBUI_HOSTNAME="openwebui"
OPENWEBUI_WEBUI_URL="https://${OPENWEBUI_HOSTNAME}.${SERVICES_DOMAIN}"
OPENWEBUI_OLLAMA_BASE_URL="http://${OPENWEBUI_OLLAMA_HOST}:${OPENWEBUI_OLLAMA_PORT}"
OPENWEBUI_WEBUI_SECRET_KEY="<secure>"
OPENWEBUI_WEBUI_NAME="Voidside AI"
OPENWEBUI_OPENROUTER_API_KEY="<API KEY>"

## GPU Configuration (for older Nvidia GPUs like Quadro P4000)
OPENWEBUI_CUDA_LAUNCH_BLOCKING=1
OPENWEBUI_TORCH_CUDA_ARCH_LIST=6.1

# SMTP Configuration
OPENWEBUI_SMTP_SERVER=smtp.protonmail.ch
OPENWEBUI_SMTP_PORT=587
OPENWEBUI_SMTP_USERNAME=truenas@darkcatalist.com
OPENWEBUI_SMTP_PASSWORD="<platform password>"
OPENWEBUI_SMTP_USE_TLS=true
OPENWEBUI_SMTP_FROM_EMAIL=truenas@darkcatalist.com
OPENWEBUI_SMTP_FROM_NAME="TrueNAS App Support - OpenWebUI"

# pgvector Database (Vector Operations)
OPENWEBUI_PGVECTOR_HOST=openwebui-pgvector
OPENWEBUI_PGVECTOR_PORT=5432
OPENWEBUI_PGVECTOR_USER=openwebui
OPENWEBUI_PGVECTOR_PASSWORD="<secure>"
OPENWEBUI_PGVECTOR_DB=openwebui
OPENWEBUI_PGVECTOR_MAX_VECTOR_LENGTH=1536
OPENWEBUI_PGVECTOR_CHUNK_SIZE=1000
OPENWEBUI_POSTGRES_MAX_CONNECTIONS=100
OPENWEBUI_UVICORN_WORKERS=4
# Vector Database Configuration
OPENWEBUI_VECTOR_DB=pgvector
OPENWEBUI_PGVECTOR_DB_URL="postgresql://${OPENWEBUI_PGVECTOR_USER}:${OPENWEBUI_PGVECTOR_PASSWORD}@${OPENWEBUI_PGVECTOR_HOST}:${OPENWEBUI_PGVECTOR_PORT}/${OPENWEBUI_PGVECTOR_DB}"
OPENWEBUI_DB_DATABASE_URL="${OPENWEBUI_PGVECTOR_DB_URL}"

# Valkey Configuration (Redis Alternative)
OPENWEBUI_VALKEY_HOST=openwebui-valkey
OPENWEBUI_VALKEY_PORT=6379
OPENWEBUI_VALKEY_PASSWORD="<secure>"
OPENWEBUI_VALKEY_URL="redis://:${OPENWEBUI_VALKEY_PASSWORD}@${OPENWEBUI_VALKEY_HOST}:${OPENWEBUI_VALKEY_PORT}/0"
REDIS_URL=${OPENWEBUI_VALKEY_URL}

# SearXNG
OPENWEBUI_SEARXNG_APP_IP=192.168.6.133
OPENWEBUI_SEARXNG_PORT=8080
# OPENWEBUI_SEARXNG_BASE_URL=https://searxng.services.darkcatalist.com
SEARXNG_BASE_URL=http://openwebui-searxng:8080
OPENWEBUI_SEARXNG_SECRET_KEY="<secure>"
OPENWEBUI_SEARXNG_SETTINGS_PATH=/etc/searxng
UWSGI_SETTINGS_PATH=/etc/searxng/uwsgi.ini

# vscode
OPENWEBUI_VSCODE_APP_IP=192.168.6.134
APPDATA_VSCODE=/mnt/zfspool/appdata/vscode
HOME_VSCODE=/mnt/zfspool/system/HOME/sganti
OPENWEBUI_VSCODE_PROXY_DOMAIN=openwebui.code.darkcatalist.com
OPENWEBUI_VSCODE_SUDO_PASSWORD="<secure>"
OPENWEBUI_VSCODE_DEFAULT_WORKSPACE=/config/workspace
OPENWEBUI_VSCODE_KEEPALIVE_INTERVAL=60
OPENWEBUI_VSCODE_KEEPALIVE_TIMEOUT=300

# Adminer
OPENWEBUI_ADMINER_APP_IP=192.168.6.135
ADMINER_PLUGINS=tables-filter tinymce
ADMINER_DEFAULT_SERVER=openwebui-pgvector

